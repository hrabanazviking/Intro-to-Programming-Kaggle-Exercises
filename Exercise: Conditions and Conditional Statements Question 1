# TODO: Edit the function to return the correct grade for different scores
def get_grade(score):
    if score < 60:
        grade = "F"
    elif score < 70:
        grade = "D"
    elif score < 80:
        grade = "C"
    elif score < 90:
        grade = "B"
    else:
        grade = "A"
    return grade
    
